{
    "start_url": "https://www.meteored.mx",
    "actions_groups": [{
    "actions": [
        {
            "action": "input",
            "by": "CSS_SELECTOR",
            "value": "#search_pc",
            "input_value": "Torreon",
            "hidden": false,
            "wait": 5
        },
        {
            "action": "keyboard",
            "input_value": "DOWN",
            "wait": 1
        },
        {
            "action": "keyboard",
            "input_value": "DOWN",
            "wait": 1
        },
        {
            "action": "keyboard",
            "input_value": "ENTER",
            "wait": 10
        },
        {
            "action": "click",
            "by": "CSS_SELECTOR",
            "value": "body > div.franjas.container > div:nth-child(1) > section > aside > ul:nth-child(1) > li:nth-child(3) > a",
            "wait": 10
        },
        {
            "action": "click",
            "by": "CSS_SELECTOR",
            "value": "#tipoResumen_2",
            "wait": 10
        },
        {
            "action": "find_elements",
            "by": "CSS_SELECTOR",
            "value": "#body_tanio > tr",
            "columns": [
                {
                    "header": "MES",
                    "selector_pattern": "td:nth-child(1)"
                },
                {
                    "header": "T. MEDIA",
                    "selector_pattern": "td:nth-child(2)"
                },
                {
                    "header": "T. MÁX",
                    "selector_pattern": "td:nth-child(3)"
                },
                {
                    "header": "T. MÍN",
                    "selector_pattern": "td:nth-child(4)"
                },
                {
                    "header": "V. MEDIA VIENTO",
                    "selector_pattern": "td:nth-child(5)"
                },
                {
                    "header": "RACHAS MÁX",
                    "selector_pattern": "td:nth-child(6)"
                },
                {
                    "header": "PRESIÓN MEDIA",
                    "selector_pattern": "td:nth-child(7)"
                }
            ],
            "wait": 1
        }
    ],
    "output": {
        "filename": "Clima.xlsx"
    }}]
}
